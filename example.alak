lapag tropa = "Leetz";
lapag ambag_one = 5;
lapag ambag_two = 150;
lapag total_ambag = 0;

total_ambag = ambag_one + ambag_two;

if total_ambag > 100 {
  shot "letsgo";
} else {
  shot "Ambag muna";
}


lapag bote = 0;
jumbo bote < 5 {
  shot "Bili na ng {bote} gin";
  bote = bote + 1;
}

fun oras_na {
  shot "Shot na {tropa} may ambag {total_ambag} pesos na!";
}

oras_na();
